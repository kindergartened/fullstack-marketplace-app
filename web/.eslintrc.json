{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": ["warn"],
        "quotes": ["warn", "double"],
        "react/prop-types": 0,
        "react/react-in-jsx-scope": 0,
        "object-curly-spacing": ["warn"],
        "indent": ["warn"]
    }
}
